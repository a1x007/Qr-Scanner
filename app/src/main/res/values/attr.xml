<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Button">
        <attr name="_radius" format="dimension" />
        <attr name="_fillColor" format="color" />
        <attr name="_borderColor" format="color" />
        <attr name="_borderWidth" format="dimension" />
        <attr name="_icon" format="reference" />
        <attr name="_iconColor" format="color" />
        <attr name="_iconWidth" format="dimension" />
        <attr name="_iconHeight" format="dimension" />
        <attr name="_textMarginTop" format="dimension" />
        <attr name="_textMarginBottom" format="dimension" />
        <attr name="_textMarginLeft" format="dimension" />
        <attr name="_textMarginRight" format="dimension" />
        <attr name="_checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Frame">
        <attr name="cornerRadius" format="dimension" />
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>

</resources>